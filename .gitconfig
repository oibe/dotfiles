[user]
    name = oibe
    email = kikeibe@gmail.com
[diff]
    tool = vimdiff
[difftool]
    prompt = false
[alias]
    d = difftool --prompt
    ca = commit --amend --no-edit
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    cmd = p4merge.exe \\\"$BASE\\\" \\\"$LOCAL\\\" \\\"$REMOTE\\\" \\\"$MERGED\\\"
    cdiff = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative # <my_branch>..<other_branch>
    tracked = ls-tree --full-tree -r HEAD
[merge]
    keepBackup = false
    tool = custom
[mergetool "custom"]
    cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge "$PWD/$BASE" "$PWD/$REMOTE" "$PWD/$LOCAL" "$PWD/$MERGED"
    keepTemporaries = false
    trustExitCode = false
    keepBackup = false

[core]
    excludesfile = /Users/Onwukike/.gitignore_global
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
